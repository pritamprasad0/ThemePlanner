<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pritamprasad.helloworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.GoalsActivity" />
        <activity android:name=".Activity.AddNewThemeActivity" />
        <activity android:name=".Activity.TasksActivity" />
        <activity android:name=".Activity.TaskDetailsActivity" />
        <activity android:name=".Activity.AddNewGoalActivity"></activity>
    </application>

</manifest>